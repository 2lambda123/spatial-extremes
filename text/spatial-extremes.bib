  journal = {Environmetrics},
  author = {Gilleland, Eric and Brown, Barbara G and Ammann, Caspar M},
  month = sep,
  year = {2013},
  pages = {418--432},
  file = {PubMed Central Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/IH96JSK8/Gilleland et al. - 2013 - Spatial extreme value analysis to project extremes.pdf:application/pdf},
  pmid = {24223482},
  pmcid = {PMC3816326}
}

