  month = dec,
  year = {2012},
  keywords = {0480 Remote sensing,4855 Phytoplankton,4858 Population dynamics and ecology,bloom timing,California Current,Oregon coast,phenology,satellite remote sensing,upwelling},
  pages = {C12002},
  file = {Full Text PDF:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/SK7FE4IS/McKibben et al. - 2012 - Satellite-based detection and monitoring of phytop.pdf:application/pdf;Snapshot:/Users/eric.ward/Library/Application Support/Zotero/Profiles/6ic0vo4n.default/zotero/storage/TR298G5E/abstract.html:text/html}
